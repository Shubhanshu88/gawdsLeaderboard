<table class="table table-hover">
  <thead class="w-100">
    <tr>
      <th class="text-center">SrNo</th>
      <th>Name</th>
      <th class="toShow" colspan="2">Other Stats</th>
    </tr>
  </thead>
  <tbody>
    <% yearThree.forEach((users) => { %>
    <tr class="row-click" data-href="/dashboard/<%= users.login %>">
      <td class="text-center">
        <%= counter[2] %>
      </td>
      <td>
        <%= users.login %>
      </td>
      <td class="toShow">Followers:
        <%= users.followers %>
      </td>
      <td class="toShow">Following:
        <%= users.following %>
      </td>
    </tr>
    <% counter[2]++ %>
    <% }) %>
  </tbody>
</table>